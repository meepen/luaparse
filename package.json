{
  "name": "@meepen/luaparse",
  "version": "1.0.10",
  "description": "A lua parser",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "!dist/utilities",
    "!dist/*.tsbuildinfo"
  ],
  "keywords": [
    "lua",
    "parser",
    "parsing",
    "luaparse",
    "ast"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/meepen/luaparse.git"
  },
  "bugs": {
    "url": "https://github.com/meepen/luaparse/issues"
  },
  "homepage": "https://meepen.dev/luaparse/",
  "type": "module",
  "scripts": {
    "prebuild": "rimraf -g dist src/**/*.js src/**/*.js.map src/**/*.d.ts",
    "build": "tsc --build tsconfig.app.json",
    "lint": "eslint .",
    "test": "vitest run",
    "test:resource": "tsx utilities/test.ts test",
    "pretest:resource:bench": "rimraf coverage/speed && mkdir -p coverage/speed && tsc --build tsconfig.utilities.json",
    "test:resource:bench": "node --cpu-prof --cpu-prof-dir=coverage/speed --enable-source-maps ./dist/utilities/test.js test --times 100",
    "posttest:resource:bench": "tsx utilities/remap-all-profiles.ts",
    "test:coverage": "vitest run --coverage",
    "coverage:badge": "node scripts/gen-coverage-badge.mjs",
    "postcoverage": "npm run coverage:badge",
    "speedtest": "tsx utilities/speed.ts"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/luaparse": "^0.2.12",
    "@types/node": "^22",
    "@types/yargs": "^17.0.33",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jiti": "^2.5.1",
    "luaparse": "^0.3.1",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "source-map": "^0.7.6",
    "tinybench": "^5.0.1",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.42.0",
    "vitest": "^3.2.4",
    "yargs": "^18.0.0"
  },
  "author": {
    "email": "meep@meepen.dev",
    "name": "Devin Korb",
    "url": "https://meepen.dev"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
      "types": "./dist/src/index.d.ts"
    },
    "./package.json": "./package.json"
  }
}
