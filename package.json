{
  "name": "@meepen/luaparse",
  "version": "1.0.9",
  "description": "A lua parser",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "lua",
    "parser",
    "ast",
    "tokenizer"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/meepen/luaparse.git"
  },
  "bugs": {
    "url": "https://github.com/meepen/luaparse/issues"
  },
  "homepage": "https://meepen.dev/luaparse/",
  "type": "module",
  "scripts": {
    "prebuild": "rimraf -g dist src/**/*.js src/**/*.js.map src/**/*.d.ts",
    "build": "tsc --build tsconfig.app.json",
    "bin": "npm run build && node --enable-source-maps dist/bin.js",
    "pretest": "eslint .",
    "test": "vitest run",
  "test:coverage": "vitest run --coverage",
  "coverage:badge": "node scripts/gen-coverage-badge.mjs",
  "postcoverage": "npm run coverage:badge"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/node": "^22",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jiti": "^2.5.1",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.42.0",
    "vitest": "^3.2.4"
  },
  "author": {
    "email": "meep@meepen.dev",
    "name": "Devin Korb",
    "url": "https://meepen.dev"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
      "types": "./dist/src/index.d.ts"
    },
    "./package.json": "./package.json"
  }
}
