{
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.detectIndentation": false,
  "files.insertFinalNewline": true,
  "files.eol": "\n",
  "files.exclude": {
    "src/**/*.js": true,
    "src/**/*.js.map": true,
    "src/**/*.d.ts": true
  },
  "search.exclude": {
    "src/**/*.js": true,
    "src/**/*.js.map": true,
    "src/**/*.d.ts": true
  },
  "files.watcherExclude": {
    "src/**/*.js": true,
    "src/**/*.js.map": true,
    "src/**/*.d.ts": true
  }
}
